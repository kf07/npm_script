  },
  "homepage": "https://github.com/kf07/npm_script#readme",
  "devDependencies": {
    "node-sass": "^4.9.3",
    "node-sass-globbing": "0.0.23",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6"
  }
}

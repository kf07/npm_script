{
  "name": "npm-script",
  "version": "1.0.0",
  "description": "\"# npm_script\"",
  "main": "index.js",
  "scripts": {
    "build:scss": "node-sass --importer node_modules/node-sass-globbing/index.js src/sass --output dist/assets/css --output-style compressed",
    "pug": "pug src/pug/index.pug -P -output ./dist/",
    "watch:scss": "node-sass src/scss/ --output dist/assets/css",
    "scss:help": "node-sass --help"
  },
